html(lang="en")
    head
        title æ ž
        meta(charset="utf-8")
        link(href="css/arranger.css", rel="stylesheet")
    body
        div#header
            h2 Shiori, is a bookmark arranger
            form#search-form
                input#search-text(type="text",placeholder="Keyword", autofocus)
                // button#search-button Search
            // button#toggle-sub 2nd panel
        div#main
            div.container.tree
                div#panel-bm-tree.panel
                    div.panel-heading Folders
                    div.panel-body#bookmarks-tree

            div.container.lists
                div#panel-bm-list.panel
                    div.panel-heading List
                    div.panel-body#bookmarks-list
                div#panel-bm-list.panel.sub
                    div.panel-heading List 2
                    div.panel-body#bookmarks-list

            div.container.tree.sub
                div#panel-bm-list.panel
                    div.panel-heading Folders 2
                    div.panel-body#bookmarks-tree2


        script(src="js/util.js")
        script(src="js/shiori.js")
        script(src="js/bmm.js")
        script(src="js/arranger.js")

